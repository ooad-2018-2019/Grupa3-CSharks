@model SeekAndFind.Models.FakultetiAplikanta

@{
    ViewData["Title"] = "Create";
}

<h2>Dodaj fakultet </h2>

<h4>FakultetiAplikanta</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FakultetId" class="control-label"></label>
                <select asp-for="FakultetId" class ="form-control" asp-items="ViewBag.FakultetId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
